<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:usn="urn://x-artefacts-gnivc-ru/ais3/USN/UsnPartnersIntegrationService/types/1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="urn://x-artefacts-gnivc-ru/ais3/USN/UsnPartnersIntegrationService/types/1.0"
           elementFormDefault="qualified" version="0.1">

    <xs:element name="UsnPlatformError">
        <xs:annotation>
            <xs:documentation>Бизнес ошибка в платформе ЦСН</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Code" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Код ошибки</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Message" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Сообщение конечному пользователю в виде шаблона с {attrKey} атрибутами
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Args" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Аргументы для сообщения пользователю</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Key" type="xs:string" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>Ключ</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Value" type="xs:string" minOccurs="1" maxOccurs="1">
                                <xs:annotation>
                                    <xs:documentation>Значение</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="TaxpayerBindStatus">
        <xs:sequence>
            <xs:element name="Id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код заявки на привязку</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Result">
                <xs:annotation>
                    <xs:documentation>Результат запроса</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="COMPLETED"/>
                        <xs:enumeration value="FAILED"/>
                        <xs:enumeration value="DUPLICATED"/>
                        <xs:enumeration value="IN_PROGRESS"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Inn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Permissions" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Список подтвержденных групп разрешений на подключения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProcessingTime" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата рассмотрения заявки НП ЦСН</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TaxpayerStatus">
        <xs:sequence>
            <xs:element name="Inn" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FirstName" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Имя контактного лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastName" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Фамилия контактного лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Patronymic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Отчество контактного лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Kpp" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>КПП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Name" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Полное название организации или ИП по ЕГРЮЛ или ЕГРИП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RegistrationTime" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата последней постановки на учёт</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UnregistrationTime" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата последнего снятия с учета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UnregistrationReason" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Причина снятия с учёта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Oktmo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Текущий ОКТМО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxObject" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Текущий объект налогообложения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Phone" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер телефона контактного лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Email" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>E-mail контактного лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UpdateTime" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата последнего обновления данных</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ContactPersonInfoUpdateTime" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата последнего обновления контактных данных</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Permissions" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Группа исключительных прав, которые в текущий момент никому не выданы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Violations" type="usn:Violation" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Коды нарушения условий применения режима АУСН. Данные актуальны на начало дня вызова метода
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OperationStatus">
        <xs:sequence>
            <xs:element name="Id" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер документа (уникальный идентификатор)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusCode" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код статуса операции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusMessage" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Описание статуса операции</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpecificItem">
        <xs:sequence>
            <xs:element name="ForeignCurrencyAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма в иностранной валюте.
                        Поле обязательно, если currencyCode самой транзакции не равен RUB и недопустимо к заполнению если currencyCode равен RUB.
                        Разделитель целой и дробной части - точка.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Сумма в рублях.
                        В случае операции в иностранной валютой, указывается соответствующая сумма в рублях. Значение должно быть со знаком плюс и отлично от 0.
                        Разделитель целой и дробной части - точка.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OperationCategory" type="xs:int" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Категория операции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OperationTaxbaseCode" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код признака отнесения операции к налоговой базе</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AccountOperation">
        <xs:annotation>
            <xs:documentation>Информация об операции по счету НП</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Id" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер документа (уникальный идентификатор для сверки и дедупликации в случае
                        повторных загрузок)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastModifiedDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата и время создания разметки.
                        Связка полей Id и LastModifiedDate определяет банковскую операцию и версию разметки.
                        Самой актуальной версией для каждой банковской операции считается версия с наибольшим
                        LastModifiedDate вне зависимости от хронологического порядка передачи данных в ПП АУСН.
                        Если поле не заполнено (настоятельно рекомендуется указывать данное поле) - то заполняется
                        серверным временем приема операции в ПП АУСН.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Uin" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AccountNumber" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Расчетный счет организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocNo" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер платежного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DrawDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата списания средств с р/с</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChargeDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата поступления средств на р/с</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма платежа в рублях</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PaymentPurpose" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Назначение платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CreatorStatus" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Статус составителя расчетного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Kbk" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель кода бюджетной классификации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Oktmo" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код ОКТМО территории, на которой мобилизуются денежные средства от уплаты налога,
                        сбора и иного платежа
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerName" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Плательщик</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerInn" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerAccount" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Расчетный счет плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerCorrAccount" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Кор. счет банка плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerBic" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Бик банка плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerBank" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Название банка плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PayerKpp" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>КПП плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Recipient" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Получатель</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientInn" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientAccount" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Расчетный счет получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientCorrAccount" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Кор. счет банка получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientBic" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Бик банка получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientBank" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Название банка получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OperationType" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Вид операции (условное обозначение (шифр) документов, проводимых по счетам в кредитных организациях в соответствии с положением ЦБ РФ N 579-П)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientKpp" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>КПП получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxEvidence" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель основания налогового платежа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxPeriod" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель налогового периода</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxDocNumber" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель номера документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxDocDate" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель даты документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CurrencyCode" type="xs:string" default="RUB" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Буквенный код валюты операции по “Общероссийский классификатор валют”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExchangeRate" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Курс ЦБ РФ валюты на момент поступления денежных средств.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ForeignCurrencyAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма в иностранной валюте</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Specifics" type="usn:SpecificItem" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Информация о разметке банковсковской транзакции</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TaxCharge">
        <xs:sequence>
            <xs:element name="TaxChargeId" type="xs:long" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Идентификатор начисления в ПП АУСН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GroupTypeCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Тип начисления (ACCRUED - Налог, ACCRUED_CORRECTION – Корректировка на увеличение, REDUCED_CORRECTION - Корректировка на уменьшение)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма начисления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DueDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Срок оплаты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxPeriodYear" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель налогового периода - год (YYYY)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxPeriodMonth" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель налогового периода - месяц</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Oktmo" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ОКТМО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Kbk" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>КБК</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PassedAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма поступивших оплат в ПП РСБ по данному начислению</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CreationDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата/Время создания</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AusnTaxReport">
        <xs:sequence>
            <xs:element name="TaxChargeId" type="xs:long" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Идентификатор начисления налога АУСН в ПП АУСН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GroupTypeCode">
                <xs:annotation>
                    <xs:documentation>Тип расчета</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ACCRUED"/>
                        <xs:enumeration value="ACCRUED_CORRECTION"/>
                        <xs:enumeration value="REDUCED_CORRECTION"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Amount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма начисления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DueDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Срок оплаты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxPeriodYear" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Показатель налогового периода - год (YYYY)</xs:documentation>
                </xs:annotation>
            </xs:element>
