            </xs:element>
            <xs:element name="Name" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Название файла с расширением</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="NotificationsResponse">
        <xs:sequence>
            <xs:element name="Inn" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="HasMore" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Есть ли еще записи по фильтру (limit, offset)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Notifications" type="usn:Notifications" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Список оповещений</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="NotificationsActionRequest">
        <xs:sequence>
            <xs:element name="Inn" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН НП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MessageIdList" type="xs:string" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Список идентификаторов оповещений в рамках одного НП
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TaxCalculationList">
        <xs:sequence>
            <xs:element name="Year" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Год</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Month" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Месяц</xs:documentation>
                </xs:annotation>
            </xs:element>

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PatentInfo">
        <xs:sequence>
            <xs:element name="KvpdpCode" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код КВПДП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KvpdpName" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Наименование КВПДП</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TaxRegimeStatus">
        <xs:sequence>
            <xs:element name="Inn" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ИНН НП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Status" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Статус нахождения на ЦСН</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="UNKNOWN"/>
                        <xs:enumeration value="CSN"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="DateFrom" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата начала применения режима налогообложения.
                        Если были случаи постановки и снятия с данного режима,
                        то указывается последняя дата постановки
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DateTo" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата окончания применения режима налогообложения,
                        если НП подал заявление на уход с режима.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OperationCancelStatus">
        <xs:sequence>
            <xs:element name="Id" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер документа (уникальный идентификатор)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusCode" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код статуса операции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusMessage" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Описание статуса операции</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LastCheck">
        <xs:annotation>
            <xs:documentation>Последний обработанный чек ККТ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="KktRegId" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Регистрационный номер ККТ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Num" type="xs:long" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер чека</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Date" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата чека</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SumsByDate">
        <xs:annotation>
            <xs:documentation>Список доходов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Date" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Дата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KKTSums" type="usn:KKTSums" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Данные по ККТ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankSums" type="usn:BankSums" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Данные от информационных партнеров</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LKSums" type="usn:LKSums" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Данные, введенные через ЛК</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="KKTSums">
        <xs:annotation>
            <xs:documentation>Данные по ККТ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="KKTRegID" type="xs:string">
                <xs:annotation>
                    <xs:documentation>РН ККТ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FiscalDriveNum" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ФН ККТ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KKTLocation" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Адрес установки ККТ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxBaseSums" type="usn:TaxBaseSums" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BankSums">
        <xs:annotation>
            <xs:documentation>Данные от информационных партнеров</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="BankCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Идентефикатор информационного партнера</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Полное наименование информационного партнера</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxBaseSums" type="usn:TaxBaseSums" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LKSums">
        <xs:annotation>
            <xs:documentation>Данные, введенные через ЛК</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TaxBaseSums" type="usn:TaxBaseSums" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TaxBaseSums">
        <xs:annotation>
            <xs:documentation>Суммы налоговой базы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="OperationTaxbaseCode" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Код признака отнесения к налоговой базе</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Sum" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>Сумма</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Estimations">
        <xs:annotation>
            <xs:documentation>Список расчетов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
<!--            Общие поля в начале-->
            <xs:element name="Month" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Месяц расчета налога</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Year" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Год расчета налога</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EffectiveTaxType" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Выбранный объект налогообложения: 2 - (“УСН-доходы”), 4 - (“УСН-доходы минус расходы”)</xs:documentation>
                </xs:annotation>
            </xs:element>
<!--            Доходы-->
            <xs:element name="IncomeTaxBaseAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по объекту налогообложения доходы (“УСН-доходы”). Обязательно к заполнению при EffectiveTaxType=2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxRateIncomeValue" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Налоговая ставка по объекту налогообложения доходы (“УСН-доходы”). Обязательно к заполнению при EffectiveTaxType=2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BankTransactionsAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по банковским операциям (“УСН-доходы”). Обязательно к заполнению при EffectiveTaxType=2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KktAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по чекам ККТ (“УСН-доходы”). Обязательно к заполнению при EffectiveTaxType=2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IncomeTaxAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налога по объекту налогообложения доходы (“УСН-доходы”). Обязательно к заполнению при EffectiveTaxType=2</xs:documentation>
                </xs:annotation>
            </xs:element>
<!--            Доходы минус расходы-->
            <xs:element name="IncomeMinusExpenseTaxBaseAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по объекту налогообложения, доходы уменьшенные на величину расходов (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxRateIncomeMinusExpenseValue" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Налоговая ставка по объекту налогообложения доходы, уменьшенные на величину расходов (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IncomeMinusExpenseBankTransactionsAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по банковским операциям (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KktExpenseAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по чекам ККТ (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CurrentLosses" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Текущие убытки (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PreviousPeriodLosses" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Убытки с прошлого периода (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TotalExpense" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма расходов минус возвраты расходов “УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IncomeMinusExpenseTaxAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налога с учетом выбранного объекта налогообложения после уменьшений (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MinimumTaxUsed" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Выбран ли минимальный налог как основной (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MinimumTaxRateAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма минимальной налоговой ставки для расчета для НП (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MinimumTaxRateValue" type="xs:decimal" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Текущая ставка в процентах для минимальной налоговой ставки (“УСН-доходы минус расходы”). Обязательно к заполнению при EffectiveTaxType=4</xs:documentation>
                </xs:annotation>
            </xs:element>
<!--            Общие поля в конце-->
            <xs:element name="TaxAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Финальная сумма налога</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InformDocumentAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма налоговой базы по операциям через ЛК</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RsbStatus" type="xs:short" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Статус для передачи в РСБ: 1 - предварительный расчет налога. Требуется отправить пользователю уведомление, что будет передача в РСБ, 2 - финальный расчет налога, 3 - корректировка финального расчета налога</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RemainderTradeFee" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма остатка торгового сбора переходящая на следующий месяц.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CurrentTradeFee" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма торгового сбора за текущий месяц</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeductedTradeFee" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Торговый сбор, на который был снижен налог</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="PostPlatformRegistrationRequest">
        <xs:annotation>
            <xs:documentation>Запрос на регистрацию информационного партнера в ПП ЦСН.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="PartnerName" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Название партнера</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="BIK" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>БИК</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="RegNum" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Регистрационный номер банка (в соответствии с документом
                            ЦБ РФ https://www.cbr.ru/banking_sector/credit/FullCoList/)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PartnerDescription" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Описание информационного партнера</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="TransitionLink" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Ссылка на приложение/сайт информационного партнера
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PartnersText" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Текст с описанием от информационного партнера</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PartnerImage" type="xs:base64Binary" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Логотип партнера. Ожидаемый логотип в формате JPEG (квадрат 1x1) размером до
                            100 КБ.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Inn" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>ИНН партнера</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Phone" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Номер телефона для связи</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="EnableEventStream" type="xs:boolean" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Включить формирование событий для “Единый опросный метод. Список событий”.
                            В случае включения данного флага, банк обязуется автоматически потреблять передаваемые события в указанном методе.
                            По умолчанию False.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="PostPlatformRegistrationResponse">
        <xs:annotation>
            <xs:documentation>Ответ на PostPlatformRegistrationRequest</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="PartnerID" type="xs:string" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Код партнера</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="RegistrationDate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Дата регистрации</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
