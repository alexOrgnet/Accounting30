                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="25"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="IncomeTypeCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код вида дохода (поле 20) согласно Указанию 5286-У ЦБРФ</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
