			ЗаполнитьЗначенияСвойств(ТаблицаДанных.Добавить(), СтрокаТЗ);
		КонецЦикла;
	КонецЕсли;

	Результат.ТаблицаДанных = ТаблицаДанных;
	
	Возврат Результат;

